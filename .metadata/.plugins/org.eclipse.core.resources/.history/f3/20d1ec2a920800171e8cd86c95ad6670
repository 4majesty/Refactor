	JavaTextTools getJavaTextTools();

	ASTProvider getASTProvider();

	MembersOrderPreferenceCache getMemberOrderPreferenceCache();

	TypeFilter getTypeFilter();

	FormToolkit getDialogsFormToolkit();

	JavaEditorTextHoverDescriptor[] getJavaEditorTextHoverDescriptors();

