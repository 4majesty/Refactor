package org.eclipse.jdt.internal.ui.commands;

import org.eclipse.core.commands.ParameterValueConversionException;
import org.eclipse.jdt.core.Signature;

public interface IJavaElementReferenceConverter {
	Object convertToObject(String parameterValue) throws ParameterValueConversionException;

	String convertToString(Object parameterValue) throws ParameterValueConversionException;
}
