package org.eclipse.jdt.internal.ui.typehierarchy;

import org.eclipse.jface.viewers.Viewer;
import org.eclipse.jdt.core.ITypeHierarchy;
import org.eclipse.jdt.core.JavaModelException;
import org.eclipse.jdt.ui.IWorkingCopyProvider;
import org.eclipse.jdt.internal.ui.JavaPlugin;

public interface IMethodsContentProvider {
	void showInheritedMethods(boolean show);

	boolean providesWorkingCopies();

	boolean isShowInheritedMethods();

	Object[] getElements(Object element);

	void inputChanged(Viewer input, Object oldInput, Object newInput);

	void dispose();
}
