public interface IBuildPathsBlock {
	Control createControl(Composite parent);

	void init(IJavaProject jproject, IPath outputLocation, IClasspathEntry[] classpathEntries);

	boolean hasChangesInDialog();

	boolean hasChangesInClasspathFile();

	boolean isClassfileMissing();

	void initializeTimeStamps();

	IJavaProject getJavaProject();

	IPath getOutputLocation();

	IClasspathEntry[] getRawClassPath();

	int getPageIndex();

	void changeControlPressed(DialogField field);

	void dialogFieldChanged(DialogField field);

	void customButtonPressed(ListDialogField field, int index);

	void doubleClicked(ListDialogField field);

	void selectionChanged(ListDialogField field);

	void updateTopButtonEnablement();

	void buildPathCustomButtonPressed(ListDialogField field, int index);

	void updateClassPathStatus();

	void configureJavaProject(IProgressMonitor monitor) throws CoreException, OperationCanceledException;

	void configureJavaProject(String newProjectCompliance, IProgressMonitor monitor) throws CoreException, OperationCanceledException;

	void setElementToReveal(IClasspathEntry entry, String attributeKey);

	void addElement(IClasspathEntry entry);

	void dispose();

	boolean isOKStatus();

	void setFocus();
}
