package com.successfactors.cdp.service.mentoring.jam;

import javax.validation.constraints.NotNull;

import com.successfactors.sca.ServiceCommand;

/**
 * API to fetch Jam OAuth 2 Token for a given userId
 * 
 * @author rsrigiri
 * 
 * SAP SuccessFactors
*/
public class GetJamAuthToken implements ServiceCommand<String> {

  @NotNull
  /** user Id requiring token */
  private String userId;
  
  public GetJamAuthToken(String userId) {
    this.userId = userId;
  }

  public String getUserId() {
    return userId;
  }
}