package org.eclipse.jdt.ui.actions;

import org.eclipse.swt.graphics.Image;
import org.eclipse.core.runtime.IStatus;
import org.eclipse.jface.viewers.IStructuredSelection;
import org.eclipse.jface.viewers.Viewer;
import org.eclipse.jface.text.ITextSelection;
import org.eclipse.jdt.internal.ui.util.ExceptionHandler;
import org.eclipse.jdt.internal.ui.viewsupport.BasicElementLabels;
import org.eclipse.jdt.internal.ui.viewsupport.JavaElementImageProvider;
import org.eclipse.jdt.internal.ui.javaeditor.ICompilationUnitEditor;

public interface IAddGetterSetterAction {
	void selectionChanged(IStructuredSelection selection);

	void run(IStructuredSelection selection);

	IStatus validate(Object[] selection);

	void selectionChanged(ITextSelection selection);

	void run(ITextSelection selection);

	String getText(Object element);

	Image getImage(Object element);

	void inputChanged(Viewer viewer, Object oldInput, Object newInput);

	Object[] getChildren(Object parentElement);

	Object getParent(Object element);

	boolean hasChildren(Object element);

	Object[] getElements(Object inputElement);

	void dispose();

	boolean select(Viewer viewer, Object parentElement, Object element);

	boolean getSortOrder();

	void setSortOrder(boolean sort);

	void allowSettersForFinals(boolean allowSettersForFinals);
}
