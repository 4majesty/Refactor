package org.eclipse.jdt.internal.corext.refactoring.rename;

import org.eclipse.jdt.core.dom.IBinding;
import org.eclipse.jdt.core.dom.Javadoc;
import org.eclipse.jdt.core.dom.SimpleName;
import org.eclipse.jdt.core.dom.VariableDeclaration;
import org.eclipse.jdt.internal.corext.dom.ASTNodes;

public interface ITempOccurrenceAnalyzer {
	void perform();

	int[] getReferenceOffsets();

	int[] getReferenceAndJavadocOffsets();

	int getNumberOfReferences();

	SimpleName[] getReferenceNodes();

	SimpleName[] getJavadocNodes();

	SimpleName[] getReferenceAndDeclarationNodes();

	boolean visit(Javadoc node);

	void endVisit(Javadoc node);

	boolean visit(SimpleName node);
}
