	/** 
	* locks down the _enumList after all the enumeration constants have been created. Subsequent calls to the constructor will fail with an UnsupportedOperationException.
	*/
	private static final void lockdown() {
		_enumList = Collections.unmodifiableList(Arrays.asList(_enumList.toArray()));
		int maxId = 0;
		for (int i = _enumList.size(); --i >= 0;) {
			final int id = ((FeatureEnum) _enumList.get(i))._id;
			if (id > maxId) {
				maxId = id;
			}
		}
		_idMap = new FeatureEnum[maxId + 1];
		for (int i = _enumList.size(); --i >= 0;) {
			final FeatureEnum feature = (FeatureEnum) _enumList.get(i);
			final int id = feature.getId();
			if (_idMap[id] != null) {
				throw new InternalError("multiple FeatureEnum elements have the same id " + id);
			}
			_idMap[id] = feature;
		}
		try {
			final Class thisClass = FeatureEnum.class;
			final int MODIFIERS = Modifier.PUBLIC | Modifier.STATIC | Modifier.FINAL;
			final Field fields[] = thisClass.getFields();
			for (final Field field : fields) {
				if ((field.getModifiers() & MODIFIERS) == MODIFIERS && field.getType() == thisClass) {
					final FeatureEnum feature = (FeatureEnum) field.get(null);
					feature._constantName = field.getName();
					_enumMap.put(feature._constantName, feature);
				}
			}
		} catch (final Exception e) {
			e.printStackTrace();
		}
	}

	/** 
	* The database unique identifier.
	*/
	private final int _id;
	/** 
	* The messages.properties key
	*/
	private final String _messageKey;
	/** 
	* The company settings .properties key (may be null)
	*/
	private final String _key;
	/** 
	* The licence product name.
	*/
	private final String _product;
	/** 
	* Is this feature visible on and hence editable through UI
	*/
	private boolean _isUIVisible = true;
	/** 
	* The name of the constant as defined in the java-name space. This value is pulled out using reflection, and is only available after the lock-down static initializer is called (e.g. not available to the constructor).
	*/
	private String _constantName;
	private final FeatureReadinessEnum _readiness;
	private List _obsoletes;
	private FeatureEnum _obsoletedBy;
	private List _requires;

	public static FeatureEnum valueOf(final String name) {
		if (_enumMap.containsKey(name)) {
			return _enumMap.get(name);
		} else {
			throw new IllegalArgumentException("No FeatureEnum const " + name + "." + name);
		}
	}

	/** 
	* Registers an Enum constant with a default readiness of "PRODUCTION".
	*/
	private FeatureEnum(final int id, final String messageKey, final String key, final String product,
			final FeatureGroupEnum fge) {
		this(id, messageKey, key, product, FeatureReadinessEnum.PRODUCTION, fge, true);
	}

	/** 
	* Registers an Enum constant with a default readiness of "PRODUCTION".
	*/
	private FeatureEnum(final int id, final String messageKey, final String key, final String product,
			final FeatureGroupEnum fge, final boolean isVisible) {
		this(id, messageKey, key, product, FeatureReadinessEnum.PRODUCTION, fge, isVisible);
	}

	/** 
	* Registers a Enum constant in the map and list.
	*/
	private FeatureEnum(final int id, final String messageKey, final String key, final String product,
			final FeatureReadinessEnum readiness, final FeatureGroupEnum fge) {
		this(id, messageKey, key, product, readiness, fge, true);
	}

	/** 
	* Registers a Enum constant in the map and list.
	*/
	private FeatureEnum(final int id, final String messageKey, final String key, final String product,
			final FeatureReadinessEnum readiness, final FeatureGroupEnum fge, final boolean isVisible) {
		if (id < 0) {
			throw new IllegalArgumentException("Invalid ID.  IDs must be > 0");
		}
		_id = id;
		_messageKey = messageKey;
		_key = key;
		_product = product;
		if (null != _productMap.get(product)) {
			throw new IllegalArgumentException("Duplicate product name: " + product + ". Product names must be unique");
		}
		_productMap.put(product, this);
		_enumList.add(this);
		_readiness = readiness;
		_isUIVisible = isVisible;
		fge.add(this);
	}

	/** 
	* Returns the feature id.
	*/
	@Override
	public int getId() {
		return _id;
	}

	/** 
	* Returns the LicenceBean Product name.
	*/
	@Override
	public String getProduct() {
		return _product;
	}

	/** 
	* Returns the name of the constant. <b>This should only be used for debug messages, since there is no obligation to keep the constant names constant.</b>
	*/
	@Override
	public String getConstantName() {
		return _constantName;
	}

	/** 
	* Returns the messages.properties key that maps to the localized name of this feature. Note, right now this is just "FEATURE_XXX" because of laziness, but should probably be something a little more readable.
	*/
	@Override
	public String getMessageKey() {
		return _messageKey;
	}

	/** 
	* Returns the .properties file key for CompanySettingsBean and CompanySetter. May be null if there is no direct mapping to a .properties file entry.
	*/
	@Override
	public String getKey() {
		return _key;
	}

	/** 
	* A hashcode. This implementation is not necessary because the Object.hashCode method will work fine for singletons, however this implementation is more predictable.
	*/
	@Override
	public int hashCode() {
		return _id;
	}

	/** 
	* @return the production readiness of this feature
	*/
	@Override
	public FeatureReadinessEnum getReadiness() {
		return _readiness;
	}

	/** 
	* @return a list of features that were obsoleted by this feature
	*/
	@Override
	public List getObsoletedFeatures() {
		return _obsoletes == null ? Collections.EMPTY_LIST : Collections.unmodifiableList(_obsoletes);
	}

	/** 
	* @return true if this feature is obsolete
	*/
	@Override
	public boolean isObsolete() {
		return _obsoletedBy != null;
	}

	/** 
	* @return the feature that obsoleted this feature, or null if this feature isnot obsolete.
	*/
	@Override
	public FeatureEnum getObsoletedBy() {
		return _obsoletedBy == this ? null : _obsoletedBy;
	}

	/** 
	* small hack. a feature that is just obsolete is marked as such by setting it to obsolete itself.
	*/
	private void obsolete() {
		_obsoletedBy = this;
	}

	/** 
	* This feature obsoletes another feature.
	* @param oldFeature the feature that is obsoleted by this feature
	*/
	private void obsoletes(final FeatureEnum oldFeature) {
		oldFeature._obsoletedBy = this;
		if (_obsoletes == null) {
			_obsoletes = new ArrayList();
		}
		_obsoletes.add(oldFeature);
	}

	/** 
	* @return the features required by this feature. Only includes the firstlevel of depenencies.
	*/
	@Override
	public List getRequiredFeatures() {
		return _requires == null ? Collections.EMPTY_LIST : Collections.unmodifiableList(_requires);
	}

	/** 
	* @return the complete list of features required for this feature. Thisincludes features required by required features, etc...
	*/
	@Override
	public List getCompleteRequiredFeatureList() {
		if (_requires == null) {
			return Collections.EMPTY_LIST;
		}
		final HashSet set = new HashSet(_requires);
		final List reqList = new ArrayList(_requires);
		for (int i = 0; i < reqList.size(); i++) {
			final FeatureEnum f = (FeatureEnum) reqList.get(i);
			final List subFeatures = f._requires;
			if (subFeatures != null) {
				for (int j = 0; j < subFeatures.size(); ++j) {
					final FeatureEnum sub = (FeatureEnum) subFeatures.get(j);
					if (!set.contains(sub)) {
						set.add(sub);
						reqList.add(sub);
					}
				}
			}
		}
		return reqList;
	}

	/** 
	* This feature requires another feature
	* @param other the required feature
	*/
	private void requires(final FeatureEnum other) {
		if (_requires == null) {
			_requires = new ArrayList();
		}
		_requires.add(other);
	}

	/** 
	* Is this feature visible and hence editable on UI.
	* @return true if this feature is visible; false otherwise
	*/
	@Override
	public boolean isVisible() {
		return _isUIVisible;
	}

	/** 
	* Returns a FeatureEnum for the given product name, or null if no such feature exists.
	*/
	public static final FeatureEnum forProduct(final String product) {
		return (FeatureEnum) _productMap.get(product);
	}

	/** 
	* Returns a FeatureEnum for the given feature id, or null if no such feature exists.
	*/
	public static final FeatureEnum forId(final int id) {
		if (id < 0 || id >= _idMap.length) {
			return null;
		}
		return _idMap[id];
	}

	/** 
	* Returns an unmodifiable list of all FeatureEnum
	*/
	public static final List list() {
		return _enumList;
	}

	/** 
	* Returns the number of FeatureEnum constants.
	*/
	public static final int count() {
		return _enumList.size();
	}

	/** 
	* Returns an unmodifiable iterator over all the FeatureEnum constants.
	*/
	public static final Iterator iterator() {
		return _enumList.iterator();
	}

	/** 
	* Returns an unmodifiable listIterator over all the FeatureEnum constants.
	*/
	public static final ListIterator listIterator() {
		return _enumList.listIterator();
	}

	/** 
	* Serialization helper, so the the original (now-locked-down) constructor doesn't get called again as part of the deserialization process. Returned replacement will deserialize to the constant object.
	*/
	protected Object writeReplace() throws ObjectStreamException {
		return new Serialization(getId());
	}

	/** 
	* Serialization helper. These objects replace the StandardElementEnum before they are placed into the serialization stream. When deserialized, they resolve themselves to the enum constants.
	*/
	static class Serialization implements Serializable {
		int _id;

		/** 
		* Serialization constructor
		*/
		public Serialization() {
		}

		/** 
		* writeReplace constructor
		*/
		public Serialization(final int id) {
			_id = id;
		}

		@Override
		public Object readResolve() throws ObjectStreamException {
			final FeatureEnum resolved = FeatureEnum.forId(_id);
			if (resolved == null) {
				throw new InvalidObjectException(
						"Unknown " + FeatureEnum.class.getName() + " constant \"" + _id + "\" encountered");
			}
			return resolved;
		}
	}
}
