public interface IExtractSupertypeProcessor {
	String getProcessorName();

	RefactoringStatus checkExtractedCompilationUnit();

	RefactoringStatus checkFinalConditions(final IProgressMonitor monitor, final CheckConditionsContext context) throws CoreException, OperationCanceledException;

	IType computeExtractedType(final String name);

	Change createChange(final IProgressMonitor monitor) throws CoreException, OperationCanceledException;

	RefactoringStatus createWorkingCopyLayer(final IProgressMonitor monitor);

	IType[] getCandidateTypes(final RefactoringStatus status, final IProgressMonitor monitor);

	Object[] getElements();

	IType getExtractedType();

	String getTypeName();

	IType[] getTypesToExtract();

	void resetChanges();

	void setTypeName(final String name);

	void setTypesToExtract(final IType[] types);
}
