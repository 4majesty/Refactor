public interface IAddDelegateMethodsAction {
	IStatus validate(Object[] selection);

	void dispose();

	Object[] getChildren(Object element);

	int getCount();

	Object[] getElements(Object inputElement);

	IVariableBinding[] getExpandedElements();

	IVariableBinding[] getInitiallySelectedElements();

	Object getParent(Object element);

	boolean hasChildren(Object element);

	void inputChanged(Viewer viewer, Object oldInput, Object newInput);

	Image getImage(Object element);

	String getText(Object element);

	int category(Object element);

	int compare(Viewer viewer, Object o1, Object o2);

	void run(IStructuredSelection selection);

	void run(ITextSelection selection);

	void selectionChanged(IStructuredSelection selection);

	void selectionChanged(ITextSelection selection);
}
