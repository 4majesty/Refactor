package com.successfactors.rbp.integration.grc.xml.binding;

import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlAttribute;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlType;
import javax.xml.bind.annotation.adapters.NormalizedStringAdapter;
import javax.xml.bind.annotation.adapters.XmlJavaTypeAdapter;

public interface IPermissionItem {
	String getPermType();

	void setPermType(String value);

	String getPermissionStringValue();

	void setPermissionStringValue(String value);

	String getPermissionLongValue();

	void setPermissionLongValue(String value);

	Message getMessage();

	void setMessage(Message value);
}