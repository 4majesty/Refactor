package com.successfactors.cdp.service.mentoring.jam;

import java.util.ArrayList;
import java.util.Collection;

import com.successfactors.cdp.bean.mentoring.MentoringProgram;
import com.successfactors.sca.ServiceCommand;

/**
 * Command to send an email for jam group add admin api failure
 * 
 * @author rsrigiri
 * 
 * SAP SuccessFactors
*/
public class SendJamGroupAddAdminFailureEmail implements ServiceCommand<Void> {

  private IMentoringProgram program;

  private Collection<String> failedUsers = new ArrayList<String>();

  public SendJamGroupAddAdminFailureEmail(MentoringProgram program, 
      Collection<String> failedUsers) {
    this.program = program;
    this.failedUsers = failedUsers;
  }

  public SendJamGroupAddAdminFailureEmail(MentoringProgram program) {
    this.program = program;
  }

  public MentoringProgram getProgram() {
    return program;
  }

  public Collection<String> getFailedUsers() {
    return failedUsers;
  }
}