package com.successfactors.rbp.integration.grc.xml.binding;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlType;

/** 
 */
@XmlAccessorType(XmlAccessType.FIELD)
@XmlType(name = "", propOrder = { "fieldId", "fieldAction", "permissionItem" })
@XmlRootElement(name = "field-permission")
public class FieldPermission implements IFieldPermission {
	@XmlElement(name = "field-id", required = true)
	protected IFieldId fieldId;
	@XmlElement(name = "field-action", required = true)
	protected IFieldAction fieldAction;
	@XmlElement(name = "permission-item", required = true)
	protected PermissionItem permissionItem;

	/** 
	* Gets the value of the fieldId property.
	* @return possible object is {@link FieldId }
	*/
	@Override
	public FieldId getFieldId() {
		return fieldId;
	}

	/** 
	* Sets the value of the fieldId property.
	* @param value allowed object is {@link FieldId }
	*/
	@Override
	public void setFieldId(FieldId value) {
		this.fieldId = value;
	}

	/** 
	* Gets the value of the fieldAction property.
	* @return possible object is {@link FieldAction }
	*/
	@Override
	public FieldAction getFieldAction() {
		return fieldAction;
	}

	/** 
	* Sets the value of the fieldAction property.
	* @param value allowed object is {@link FieldAction }
	*/
	@Override
	public void setFieldAction(FieldAction value) {
		this.fieldAction = value;
	}

	/** 
	* Gets the value of the permissionItem property.
	* @return possible object is {@link PermissionItem }
	*/
	@Override
	public PermissionItem getPermissionItem() {
		return permissionItem;
	}

	/** 
	* Sets the value of the permissionItem property.
	* @param value allowed object is {@link PermissionItem }
	*/
	@Override
	public void setPermissionItem(PermissionItem value) {
		this.permissionItem = value;
	}
}
