package com.successfactors.pmcommon.bean;

import java.util.Iterator;
import com.successfactors.legacy.bean.provisioning.FeatureEnum;

public interface IFeatureSet {
	int getOldStyleBits();

	void setOldStyleBits(int bits);

	String getBitsString();

	void setBitsString(String bitsString);

	void add(FeatureEnum argEnum);

	void remove(FeatureEnum argEnum);

	void set(FeatureEnum feature, boolean b);

	boolean contains(FeatureEnum argEnum);

	Iterator iterator();

	boolean equals(Object other);

	String toString();

	Object clone();
}