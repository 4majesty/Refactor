import org.eclipse.jdt.internal.ui.JavaPlugin;

public interface IBuildPathBasePage {
	List getSelection();

	void setSelection(List selection, boolean expand);

	void addElement(CPListElement element);

	boolean isEntryKind(int kind);

	void init(IJavaProject javaProject);

	Control getControl(Composite parent);

	void setFocus();
}
