package com.successfactors.cdp.service;

import com.successfactors.sca.ServiceCommand;

import java.util.Date;

/**
 * Created by frank on 11/15/16.
 */
public class UpdateMentorUnavailableEndDate implements ServiceCommand<Void> {
    private String programId;
    private String mentorId;
    private Date unavailableEndDate;

    public String getProgramId() {
        return programId;
    }

    public void setProgramId(String programId) {
        this.programId = programId;
    }

    public String getMentorId() {
        return mentorId;
    }

    public void setMentorId(String mentorId) {
        this.mentorId = mentorId;
    }

    public Date getUnavailableEndDate() {
        return unavailableEndDate;
    }

    public void setUnavailableEndDate(Date unavailableEndDate) {
        this.unavailableEndDate = unavailableEndDate;
    }

    public static enum UnavailableEndDataChangeType {
        NO_CHANGE, ENABLED, DISABLED, CHANGED
    }
}
