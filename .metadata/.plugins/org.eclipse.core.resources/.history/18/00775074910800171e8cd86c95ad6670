public interface IJARFileSelectionDialog {
	Image getImage(Object element);

	String getText(Object element);

	Object[] getChildren(Object parentElement);

	Object getParent(Object element);

	boolean hasChildren(Object element);

	Object[] getElements(Object element);

	void dispose();

	void inputChanged(Viewer viewer, Object oldInput, Object newInput);

	boolean select(Viewer viewer, Object parent, Object element);

	int category(Object element);

	IStatus validate(Object[] selection);
}
