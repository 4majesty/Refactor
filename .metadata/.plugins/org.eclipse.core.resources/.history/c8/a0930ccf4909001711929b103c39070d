package com.successfactors.cdp.bean;

public interface IUserReadinessVO {
	String getUserId();

	void setUserId(String userId);

	String getUserName();

	void setUserName(String userName);

	long getRoleId();

	void setRoleId(long roleId);

	double getReadiness();

	void setReadiness(double readiness);

	boolean isDisplayReadiness();

	void setDisplayReadiness(boolean displayReadiness);

	String getUserTitle();

	void setUserTitle(String userTitle);

	int compareTo(UserReadinessVO o);

	int hashCode();

	boolean equals(Object obj);
}