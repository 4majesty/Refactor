package com.successfactors.cdp.bean;

import java.io.Serializable;
import java.util.Set;

/**
 * Bean map to SUGGESTED_ROLES_LIBRARY_MAP table.
 * @author I309618
 *
 */
public class SuggestedRolesSubItemBean implements Serializable {
  private String weightKey;
  private String itemKey;
  private boolean enabled;
  private Set<SuggestedRolesSubItemBean> subItems;

  public SuggestedRolesSubItemBean() {
    super();
  }

  public SuggestedRolesSubItemBean(String weightKey, 
      String itemKey, boolean enabled) {
    super();
    this.weightKey = weightKey;
    this.itemKey = itemKey;
    this.enabled = enabled;
  }

  public String getWeightKey() {
    return weightKey;
  }

  public void setWeightKey(String weightKey) {
    this.weightKey = weightKey;
  }

  public String getItemKey() {
    return itemKey;
  }

  public void setItemKey(String itemKey) {
    this.itemKey = itemKey;
  }

  public boolean isEnabled() {
    return enabled;
  }

  public void setEnabled(boolean enabled) {
    this.enabled = enabled;
  }

  public Set<SuggestedRolesSubItemBean> getSubItems() {
    return subItems;
  }

  public void setSubItems(Set<SuggestedRolesSubItemBean> subItems) {
    this.subItems = subItems;
  }

}
