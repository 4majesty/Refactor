package org.eclipse.jdt.ui;

import org.eclipse.swt.graphics.Image;
import org.eclipse.core.resources.IStorage;
import org.eclipse.jface.viewers.LabelProvider;
import org.eclipse.jface.viewers.StyledString;
import org.eclipse.jface.viewers.DelegatingStyledCellLabelProvider.IStyledLabelProvider;
import org.eclipse.jdt.internal.ui.viewsupport.JavaElementImageProvider;
import org.eclipse.jdt.internal.ui.viewsupport.StorageLabelProvider;
import org.eclipse.jdt.internal.ui.viewsupport.IJavaElementImageProvider;
import org.eclipse.jdt.internal.ui.viewsupport.IStorageLabelProvider;

public interface IJavaElementLabelProvider {
	void turnOn(int flags);

	void turnOff(int flags);

	Image getImage(Object element);

	String getText(Object element);

	StyledString getStyledText(Object element);

	void dispose();
}
