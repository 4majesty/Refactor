public interface IMentoringProgramMentorRequest {
	String getRejectComment();

	void setRejectComment(String rejectComment);

	Long getRowId();

	void setRowId(Long rowId);

	String getExternalId();

	void setExternalId(String externalCode);

	Long getInternalId();

	void setInternalId(Long internalId);

	String getMentor();

	void setMentor(String mentorUserId);

	String getMentee();

	void setMentee(String menteeUserId);

	String getProgramId();

	void setProgramId(String programId);

	MentorRequestStatusEnum getMentoringRequestStatus();

	void setMentoringRequestStatus(MentorRequestStatusEnum mentoringRequestStatus);

	Date getAcceptDate();

	void setAcceptDate(Date acceptDate);

	Date getRejectDate();

	void setRejectDate(Date rejectDate);

	Float getMatchingScore();

	void setMatchingScore(Float matchingScore);

	MatchingScoreTypeEnum getMatchingCategory();

	void setMatchingCategory(MatchingScoreTypeEnum matchingCategory);

	List<MentoringProgramSignupFormSection> getMentoringProgramMatchedArea();

	void setMentoringProgramMatchedArea(List<MentoringProgramSignupFormSection> mentoringProgramMatchedArea);
}