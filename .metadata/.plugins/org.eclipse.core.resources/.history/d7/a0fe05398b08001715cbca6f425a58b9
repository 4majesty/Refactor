package com.successfactors.rbp.integration.grc.xml.binding;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlType;

/** 
 */
@XmlAccessorType(XmlAccessType.FIELD)
@XmlType(name = "", propOrder = { "permissionItem" })
@XmlRootElement(name = "write-permission")
public class WritePermission implements IWritePermission {
	@XmlElement(name = "permission-item", required = true)
	protected PermissionItem permissionItem;

	/** 
	* Gets the value of the permissionItem property.
	* @return possible object is {@link PermissionItem }
	*/
	@Override
	public PermissionItem getPermissionItem() {
		return permissionItem;
	}

	/** 
	* Sets the value of the permissionItem property.
	* @param value allowed object is {@link PermissionItem }
	*/
	@Override
	public void setPermissionItem(PermissionItem value) {
		this.permissionItem = value;
	}
}
