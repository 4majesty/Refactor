package org.eclipse.jdt.internal.ui.propertiesfileeditor;

import org.eclipse.core.runtime.CoreException;
import org.eclipse.jface.resource.ImageDescriptor;
import org.eclipse.jface.text.IRegion;
import org.eclipse.search.core.text.TextSearchMatchAccess;
import org.eclipse.jdt.internal.ui.util.ExceptionHandler;
import org.eclipse.jdt.internal.ui.util.PatternConstructor;
import org.eclipse.jdt.internal.ui.viewsupport.BasicElementLabels;

public interface IPropertyKeyHyperlink {
	Object getAdapter(Class adapter);

	Object[] getChildren(Object o);

	ImageDescriptor getImageDescriptor(Object object);

	String getLabel(Object o);

	Object getParent(Object o);

	int compareTo(Object o);

	boolean acceptPatternMatch(TextSearchMatchAccess matchAccess) throws CoreException;

	IRegion getHyperlinkRegion();

	void open();

	String getTypeLabel();

	String getHyperlinkText();
}
