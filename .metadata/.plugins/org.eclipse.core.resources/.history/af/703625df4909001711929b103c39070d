package com.successfactors.cdp.service;
import java.util.List;
import com.successfactors.cdp.bean.CareerWorksheetSuggestedRoleVO;
import com.successfactors.sca.ServiceQuery;

/**
 * SCA to get career worksheet role page
 * 
 * @author haixunlu
 * 
 */
public class GetCareerWorksheetSuggestedRoles implements
        ServiceQuery<List<CareerWorksheetSuggestedRoleVO>> {
    private long templateId;
    private String userId;

    public GetCareerWorksheetSuggestedRoles() {
        super();
    }

    public long getTemplateId() {
        return templateId;
    }

    public void setTemplateId(long templateId) {
        this.templateId = templateId;
    }

    public String getUserId() {
        return userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
    }

}
