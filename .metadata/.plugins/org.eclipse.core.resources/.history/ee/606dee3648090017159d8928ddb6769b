package com.successfactors.cdp.bean.mentoring.vo;

import com.successfactors.cdp.service.util.mentoring.ProgramParticipantTypeEnum;

import java.io.Serializable;

/**
 * Program Participant VO
 * @author I309618
 *
 */
public class ProgramParticipantVO implements Serializable {
  private Long rowId;
  private ProgramParticipantTypeEnum type;
  private String userId;
  private String userName;
  private int threshold;
  private String externalId;

  public Long getRowId() {
    return rowId;
  }
  public void setRowId(Long rowId) {
    this.rowId = rowId;
  }
  public ProgramParticipantTypeEnum getType() {
    return type;
  }
  public void setType(ProgramParticipantTypeEnum type) {
    this.type = type;
  }
  public String getUserId() {
    return userId;
  }
  public void setUserId(String userId) {
    this.userId = userId;
  }
  public String getUserName() {
    return userName;
  }
  public void setUserName(String userName) {
    this.userName = userName;
  }

  public int getThreshold() {
    return threshold;
  }

  public void setThreshold(int threshold) {
    this.threshold = threshold;
  }

  public String getExternalId() {
    return externalId;
  }

  public void setExternalId(String externalId) {
    this.externalId = externalId;
  }
}
