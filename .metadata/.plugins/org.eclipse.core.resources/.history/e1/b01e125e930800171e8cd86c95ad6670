package org.eclipse.jdt.internal.ui.text;

import java.text.CharacterIterator;
import org.eclipse.core.runtime.Assert;

public interface ISequenceCharacterIterator {
	char first();

	char last();

	char current();

	char next();

	char previous();

	char setIndex(int position);

	int getBeginIndex();

	int getEndIndex();

	int getIndex();

	Object clone();
}
