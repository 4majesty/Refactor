import org.eclipse.jface.viewers.ISelection;
import org.eclipse.ui.IWorkbenchWindow;
import org.eclipse.ui.IWorkbenchWindowActionDelegate;

public interface ICreateRefactoringScriptAction {
	void dispose();

	void init(final IWorkbenchWindow window);

	void run(final IAction a);

	void selectionChanged(final IAction action, final ISelection selection);
}
