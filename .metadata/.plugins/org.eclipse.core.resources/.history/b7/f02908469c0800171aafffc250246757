package com.successfactors.cdp.bean.mentoring.vo;

import java.util.List;

import com.successfactors.cdp.service.util.mentoring.ProgramParticipantTypeEnum;

public class UserProfileVO {
  private String userId;
  private String fullname;
  private String userName;
  private String jobFamily;
  private String jobTitle;
  private int threshold;
  List<ProgramSignupFormSectionVO> preferences;
  ProgramParticipantTypeEnum type;
  
  public String getUserId() {
    return userId;
  }

  public void setUserId(String userId) {
    this.userId = userId;
  }

  public String getFullname() {
    return fullname;
  }

  public void setFullname(String fullname) {
    this.fullname = fullname;
  }

  public String getUserName() {
    return userName;
  }

  public void setUserName(String userName) {
    this.userName = userName;
  }

  public String getJobFamily() {
    return jobFamily;
  }

  public void setJobFamily(String jobFamily) {
    this.jobFamily = jobFamily;
  }

  public String getJobTitle() {
    return jobTitle;
  }

  public void setJobTitle(String jobTitle) {
    this.jobTitle = jobTitle;
  }

  public int getThreshold() {
    return threshold;
  }

  public void setThreshold(int threshold) {
    this.threshold = threshold;
  }

  public List<ProgramSignupFormSectionVO> getPreferences() {
    return preferences;
  }

  public void setPreferences(List<ProgramSignupFormSectionVO> preferences) {
    this.preferences = preferences;
  }

  public ProgramParticipantTypeEnum getType() {
    return type;
  }

  public void setType(ProgramParticipantTypeEnum type) {
    this.type = type;
  }
}
