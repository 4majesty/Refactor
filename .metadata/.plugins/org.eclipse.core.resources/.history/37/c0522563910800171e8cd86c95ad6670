import org.eclipse.ui.texteditor.ITextEditor;

public interface IPropertyKeyHyperlinkDetector {
	IHyperlink[] detectHyperlinks(ITextViewer textViewer, IRegion region, boolean canShowMultipleHyperlinks);
}
