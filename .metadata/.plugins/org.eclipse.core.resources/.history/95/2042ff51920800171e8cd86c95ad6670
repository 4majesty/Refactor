package org.eclipse.jdt.internal.ui.dialogs;

import org.eclipse.jdt.core.IJavaElement;
import org.eclipse.jdt.internal.ui.javaeditor.ICompilationUnitEditor;

public interface ISourceActionDialog {
	boolean close();

	void setSize(int width, int height);

	void setCommentString(String string);

	boolean getGenerateComment();

	int getVisibilityModifier();

	void setGenerateComment(boolean comment);

	void create();

	boolean getFinal();

	boolean getSynchronized();

	boolean isFinal();

	boolean isSynchronized();

	void setElementPositionEnabled(boolean enabled);

	boolean isElementPositionEnabled();

	IJavaElement getElementPosition();

	int getInsertOffset();
}
