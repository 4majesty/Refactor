package com.successfactors.cdp.service.mentoring.jam;

import javax.validation.constraints.NotNull;

import com.successfactors.cdp.bean.mentoring.jam.Member;
import com.successfactors.sca.ServiceQuery;

/**
 * Find Jam MemberId by SyncId API
 * 
 * @author rsrigiri
 * 
 * SAP SuccessFactors
*/
public class FindJamMemberBySyncId implements ServiceQuery<Member> {

  @NotNull
  /** User Id **/
  private String userId;

  /** access token to be used for the API. 
   * if this is null, then token will be generated for login user. **/
  private String accessToken;

  public FindJamMemberBySyncId(String userId) {
    this.userId = userId;
  }

  public FindJamMemberBySyncId(String userId, String accessToken) {
    this.userId = userId;
    this.accessToken = accessToken;
  }

  public String getUserId() {
    return userId;
  }

  public String getAccessToken() {
    return accessToken;
  }
}