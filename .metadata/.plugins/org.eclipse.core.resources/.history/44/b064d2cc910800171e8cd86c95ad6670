public interface IJavaSynchronizationContentProvider {
	void diffsChanged(final IDiffChangeEvent event, final IProgressMonitor monitor);

	Object[] getElements(Object parent);

	void getPipelinedChildren(final Object parent, final Set children);

	void getPipelinedElements(final Object element, final Set elements);

	Object getPipelinedParent(final Object element, final Object parent);

	boolean hasChildren(final Object element);

	PipelinedShapeModification interceptAdd(final PipelinedShapeModification modification);

	boolean interceptRefresh(final PipelinedViewerUpdate update);

	PipelinedShapeModification interceptRemove(final PipelinedShapeModification modification);

	boolean interceptUpdate(final PipelinedViewerUpdate anUpdateSynchronization);
}
